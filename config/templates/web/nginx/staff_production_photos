server {
  listen              8004;
  server_name         staff;
  root /home/deployer/staff_production/shared/public;

  location / {
    gzip_static       on;
    add_header        Cache-Control public;

    open_file_cache          max=1000 inactive=500s;
    open_file_cache_valid    600s;
    open_file_cache_errors   on;
    break;
  }

  access_log        /home/deployer/staff_production/shared/log/access-photos.log;
  error_log         /home/deployer/staff_production/shared/log/error-photos.log;
}

